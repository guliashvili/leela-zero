{
  "name": "client",
  "version": "1.0.0",
  "description": "Go game client",
  "main": "index.cjs",
  "type": "module",
  "scripts": {
    "build-dev": "webpack --config webpack.config.cjs --mode=development",
    "build-prod": "webpack --config webpack.config.cjs --mode=production",
    "lint": "tsc --noEmit && eslint '**/*.{js,ts,tsx,cjs}' --quiet --fix"
  },
