    "express": "latest",
    "immer": "latest",
    "konva": "latest",
    "lodash": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-konva": "latest",
    "use-image": "latest"
  },
  "devDependencies": {
    "@types/express": "latest",
    "@types/lodash": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "css-loader": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-react": "latest",
    "file-loader": "latest",
    "gts": "latest",
    "html-webpack-plugin": "latest",
    "lint-staged": "latest",
    "mini-css-extract-plugin": "latest",
    "prettier": "latest",
    "source-map-loader": "latest",
    "style-loader": "latest",
    "stylelint": "latest",
    "ts-loader": "latest",
    "typescript": "latest",
    "webpack": "latest",
    "webpack-cli": "latest",
    "webpack-dev-server": "latest",
    "webpack-node-externals": "latest"
  }
}
