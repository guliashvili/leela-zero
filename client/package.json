{
  "name": "client",
  "version": "1.0.0",
  "description": "Go game client",
  "main": "index.cjs",
  "type": "module",
  "scripts": {
    "build": "webpack -p --config webpack.config.cjs",
    "start": "webpack-dev-server -d --content-base ./public --config webpack.config.cjs",
    "watch": "webpack --watch --config webpack.config.cjs",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "author": "Giorgi Guliashvili",
  "license": "ISC",
  "dependencies": {
    "axios": "latest",
    "express": "latest",
    "lodash-es":  "latest",
    "react": "latest",
    "react-dom": "latest",
    "socket.io": "latest",
    "socket.io-client": "latest"
  },
  "devDependencies": {
    "stylelint": "latest",
    "eslint-plugin-react": "latest",
    "webpack-dev-server": "latest",
    "@types/lodash": "latest",
    "@types/lodash-es": "latest",
    "@types/socket.io": "latest",
    "@types/socket.io-client": "latest",
    "@types/express": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "source-map-loader": "latest",
    "ts-loader": "latest",
    "typescript": "latest",
    "webpack": "latest",
    "webpack-cli": "latest"
  }
}
